#!/usr/bin/env bash

# Make vim the default editor.
export EDITOR='code';

export KUBETAIL_SINCE=10m
export HOMEBREW_BUNDLE_FILE=/Users/sidv/Brewfile

# Prefer US English and use UTF-8.
export LANG='en_US.UTF-8';
